hd=default,user=root,shell=ssh,vdbench=/root/yoon/vdbench
hd=storage4,system=storage4
hd=storage5,system=storage5

fsd=fsd1,anchor=/nas/testv/a,depth=2,width=8,file=8,size=10MB
fsd=fsd2,anchor=/nas/testv/b,depth=2,width=8,file=16,size=10MB

fwd=fwd_rnd1,host=storage4,fsd=fsd1,operation=create,xfersize=8k,fileio=random,threads=16
fwd=fwd_rnd2,host=storage5,fsd=fsd2,operation=create,xfersize=8k,fileio=random,threads=2

fwd=fwd_sq1,host=storage4,fsd=fsd1,operation=write,xfersize=256k,fileio=sequential,threads=2
fwd=fwd_sq2,host=storage5,fsd=fsd2,operation=write,xfersize=256k,fileio=sequential,threads=16

rd=rd1,fwd=(fwd_rnd1,fwd_rnd2),fwdrate=1000,format=yes,interval=1
rd=rd2,fwd=(fwd_sq1,fwd_sq2),fwdrate=1000,format=no,interval=1,elapsed=60
