# file creation prior to IO generation
hd=default,user=root,shell=ssh,vdbench=/root/yoon/vdbench
hd=storage4,system=storage4
hd=storage5,system=storage5

fsd=fsd_$host,anchor=/nas/testv2/$host,depth=2,width=8,file=8,size=10MB,openflag=directio
#fsd=fsd_$host,host=$host,anchor=/nas/testv2/$host,depth=2,width=8,file=8,size=10MB,openflag=directio
#fsd=fsd1,anchor=/nas/testv1/a,depth=2,width=8,file=16,size=10MB,openflag=directio
#fsd=fsd2,anchor=/nas/testv1/b,depth=2,width=8,file=16,size=10MB,openflag=directio

# file creation prior to IO generation
fwd=fwd_prep_$host,host=$host,fsd=fsd_$host,operation=create,fileio=sequential,threads=8
#fwd=fwd_prep1,host=$host,fsd=fsd_$host,operation=create,fileio=sequential,threads=8

# max iops, random 8kb
fwd=fwd_rnd1,host=storage4,fsd=fsd1,operation=read,rdpct=70,fileselect=random,fileio=random,xfersize=8k,threads=16
fwd=fwd_rnd2,host=storage5,fsd=fsd2,operation=read,rdpct=70,fileselect=random,fileio=random,xfersize=8k,threads=16

#rd=rd_prep_$host,fwd=fwd_$host,fwdrate=max,format=yes,interval=1
rd=rd_prep,fwd=fwd_prep_*,fwdrate=max,format=yes,interval=1
#rd=rd_prep,fwd=(fwd_prep1,fwd_prep2),fwdrate=max,format=yes,interval=1
#rd=rd_rnd,fwd=(fwd_rnd1,fwd_rnd2),fwdrate=max,format=no,interval=1,elapsed=600
